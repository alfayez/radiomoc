  REAL32 gaussGain    :
  REAL32 recvThresh   :
  
  REAL32 seedValG    :
  REAL32 meanValG    :
  REAL32 stdValG     :
  SEQ
    -- We're rcv/fw radio configuration parameters through the channel

    inC  ? dataName
    in2C ? dataName
    inC  ? samplingRate    
    in2C ? samplingRate    
    inC  ? samplingRate2
    in2C ? samplingRate2    
    inC  ? carrierFreq
    in2C ? carrierFreq
    inC  ? rcFiltCoeff
    in2C ? rcFiltCoeff
    inC  ? dataGain
    in2C ? dataGain
    inC  ? rfGain
    in2C ? rfGain
    inC  ? rfGain2
    in2C ? rfGain2    
    inC  ? bbGain
    in2C ? bbGain
    inC  ? bbGain2
    in2C ? bbGain2
    inC  ? excessBw
    in2C ? excessBw
    inC  ? gaussGain
    in2C ? gaussGain    
    inC  ? recvThresh
    in2C ? recvThresh    
    inC  ? seedValG
    in2C ? seedValG
    inC  ? meanValG
    in2C ? meanValG
    inC  ? stdValG
    in2C ? stdValG    
    
    
    outC     ! dataName
    outC     ! samplingRate
    outC     ! samplingRate2
    outC     ! carrierFreq    
    outC     ! rcFiltCoeff
    outC     ! dataGain
    outC     ! rfGain
    outC     ! rfGain2    
    outC     ! bbGain
    outC     ! bbGain2
    outC     ! excessBw
    outC     ! gaussGain    
    outC     ! recvThresh
    outC     ! seedValG
    outC     ! meanValG
    outC     ! stdValG    
    
    DTopC ! rfGain
:
PROC DoutputToScreenTop(CHAN BYTE out!,
                       CHAN REAL32 bbScale2C?, CHAN REAL32 rrcFilt2C?,   CHAN REAL32 demodulatorC?,
                       CHAN REAL32 dataC?,     CHAN REAL32 decC?,      CHAN REAL32 rfInC?)  
  
  REAL32 txParam :
  SEQ
    rfInC ? txParam
    out.string("*tRF Input*t", 0, out!)
    out.string("*n", 0, out!)            

    bbScale2C ? txParam
    out.string("*tRF Scale*t", 0, out!)
    out.string("*n", 0, out!)            

    rrcFilt2C ? txParam
    out.string("*tRRC Filt*t", 0, out!)
    out.string("*n", 0, out!)

    demodulatorC ? txParam
    out.string("*tDemodulator*t", 0, out!)
    out.string("*n", 0, out!)    

    decC ? txParam
    out.string("*tPacket Decoder*t", 0, out!)
    out.string("*n", 0, out!)    

    dataC ? txParam
    out.string("*tData Out*t", 0, out!)
    out.string("*n", 0, out!)    

:
PROC parameterGen(CHAN REAL32 paramOC!)
  REAL32 dataName   :
  REAL32 samplingRate :
  REAL32 samplingRate2:  
  REAL32 carrierFreq  :
  REAL32 rcFiltCoeff  :
  REAL32 dataGain  :
  REAL32 rfGain       :
  REAL32 rfGain2       :
  REAL32 bbGain       :
  REAL32 bbGain2       :  
  REAL32 excessBw :
  REAL32 gaussGain    :
  REAL32 recvThresh   :
  
  REAL32 seedValG     :
  REAL32 meanValG     :
  REAL32 stdValG      :
  SEQ
    dataName      := 1.0
    samplingRate  := 320000.0
    samplingRate2 := 640000.0
    carrierFreq   := 462562500.0
    rcFiltCoeff   := 1.0
    dataGain      := 10.0
    rfGain        := 1.0
    rfGain2       := 25.0
    bbGain        := 0.3
    bbGain2       := 1.0
    excessBw      := 0.35
    gaussGain     := 0.3
    recvThresh    := 0.3

    seedValG      := 0.0
    meanValG      := 0.0
    stdValG       := 0.40
  
    paramOC ! dataName
    paramOC ! samplingRate
    paramOC ! samplingRate2
    paramOC ! carrierFreq
    paramOC ! rcFiltCoeff
    paramOC ! dataGain
    paramOC ! rfGain
    paramOC ! rfGain2
    paramOC ! bbGain
    paramOC ! bbGain2
    paramOC ! excessBw
    paramOC ! gaussGain
    paramOC ! recvThresh
    
    paramOC ! seedValG
    paramOC ! meanValG
    paramOC ! stdValG
    
:

