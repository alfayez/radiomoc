-- Additive Channel
PROC add(CHAN REAL32 outC!, CHAN REAL32 DTopC!, CHAN REAL32 inC?, CHAN REAL32 in2C?)
  REAL32 dataName  :
  REAL32 samplingRate:
  REAL32 samplingRate2:  
  REAL32 carrierFreq :
  REAL32 rcFiltCoeff :  -- provides the name of the file with the coeff rcFilt<int>.dat
  REAL32 dataGain :
  REAL32 rfGain      :
  REAL32 rfGain2     :
  REAL32 bbGain       :
  REAL32 bbGain2      :  
