-- Define the RF Out Block
PROC rfOut(CHAN REAL32 outC!, CHAN REAL32 DTopC!, CHAN REAL32 inC?)
  REAL32 symbolTime   :
  REAL32 samplingRate :
  REAL32 samplingRate2:  
  REAL32 carrierFreq  :
  REAL32 carrierPhase :
  REAL32 rcFiltCoeff  :  -- provides the name of the file with the coeff rcFilt<int>.dat
  REAL32 carrierGain  :
  REAL32 rfGain       :
  REAL32 rfGain2      :
  REAL32 gaussGain    :
  REAL32 recvThresh   :
  
  REAL32 seedValG    :
  REAL32 meanValG    :
  REAL32 stdValG     :
  
  SEQ
    inC  ? symbolTime
    inC  ? samplingRate
    inC  ? samplingRate2
    inC  ? carrierFreq
    inC  ? rcFiltCoeff
    inC  ? carrierGain
    inC  ? rfGain
    inC  ? rfGain2
    inC  ? carrierPhase
    inC  ? gaussGain
    inC  ? recvThresh
    inC  ? seedValG
    inC  ? meanValG
    inC  ? stdValG
    
    outC ! symbolTime
    outC ! samplingRate
    outC ! samplingRate2
    outC ! carrierFreq
    outC ! rcFiltCoeff
    outC ! carrierGain
    outC ! rfGain
    outC ! rfGain2
    outC ! carrierPhase
    outC ! gaussGain
    outC ! recvThresh
    outC ! seedValG
    outC ! meanValG
    outC ! stdValG
    
    DTopC ! rfGain
:
