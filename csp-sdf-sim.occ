PROC DoutputToScreenTop(CHAN BYTE out!,
                       CHAN REAL32 dataSrcC?,                  CHAN REAL32 dbpskModC?,      
                       CHAN REAL32 rrcFiltC,                   CHAN REAL32 rfScaleC?,
                       CHAN REAL32 gaussC?,                    CHAN REAL32 gaussScaleC?, CHAN REAL32 addC?, 
                       CHAN REAL32 rfScale2C?,                 CHAN REAL32 rrcFilt2C?,   CHAN REAL32 demodulatorC?,  
                       CHAN REAL32 dataC?) 
  
  REAL32 txParam :
  SEQ

    dataSrcC ? txParam
    out.string("*tData Source*t", 0, out!)
    out.string("*n", 0, out!)
    
    dbpskModC ? txParam
    out.string("*tDBPSK Modulator*t", 0, out!)
    out.string("*n", 0, out!)
    
    rrcFiltC ? txParam
    out.string("*tChannel Filter*t", 0, out!)
    out.string("*n", 0, out!)

    rfScaleC ? txParam
    out.string("*tRF Scaling*t", 0, out!)
    out.string("*n", 0, out!)    
    
    gaussC ? txParam
    out.string("*tGauss*t", 0, out!)
    out.string("*n", 0, out!)    

    gaussScaleC ? txParam
    out.string("*tGauss Scale*t", 0, out!)
    out.string("*n", 0, out!)    

    addC ? txParam
    out.string("*tAdditive Channel*t", 0, out!)
    out.string("*n", 0, out!)    

    rfScale2C ? txParam
    out.string("*tRF Scale*t", 0, out!)
    out.string("*n", 0, out!)            

    rrcFilt2C ? txParam
    out.string("*tRRC Filt*t", 0, out!)
    out.string("*n", 0, out!)

    demodulatorC ? txParam
    out.string("*tDemodulator*t", 0, out!)
    out.string("*n", 0, out!)    

    dataC ? txParam
    out.string("*tData Out*t", 0, out!)
    out.string("*n", 0, out!)    
:

PROC parameterGen(CHAN REAL32 paramOC!)
  REAL32 symbolTime   :
  REAL32 samplingRate :
  REAL32 samplingRate2:  
  REAL32 carrierFreq  :
  REAL32 rcFiltCoeff  :
  REAL32 carrierGain  :
  REAL32 rfGain       :
  REAL32 rfGain2       :
  REAL32 carrierPhase :
  REAL32 gaussGain    :
  REAL32 recvThresh   :
  
  REAL32 seedValG     :
  REAL32 meanValG     :
  REAL32 stdValG      :
  SEQ
    symbolTime    := 2.0
    samplingRate  := 16000.0
    samplingRate2 := 256000.0
    carrierFreq   := 4.0
    rcFiltCoeff   := 1.0
    carrierGain   := 1.0
    rfGain        := 30.0
    rfGain2       := 1.0
    carrierPhase  := 0.0  
    gaussGain     := 0.3
    recvThresh    := 0.3

    seedValG      := 0.0
    meanValG      := 0.0
    stdValG       := 0.40
  
    paramOC ! symbolTime
    paramOC ! samplingRate
    paramOC ! samplingRate2
    paramOC ! carrierFreq
    paramOC ! rcFiltCoeff
    paramOC ! carrierGain
    paramOC ! rfGain
    paramOC ! rfGain2
    paramOC ! carrierPhase
    paramOC ! gaussGain
    paramOC ! recvThresh
    
    paramOC ! seedValG
    paramOC ! meanValG
    paramOC ! stdValG
    
:

PROC main(CHAN BYTE out!)
  CHAN REAL32 scaleToTxC      :
  CHAN REAL32 dataToTxC       :
  CHAN REAL32 txToChanFiltC   :
  CHAN REAL32 filterToScaleC  :  
  CHAN REAL32 scaleToAddC   :

  
  CHAN REAL32 rfInToScaleC    :
  CHAN REAL32 scaleToRrcFiltC :
  CHAN REAL32 rrcFiltToDemodC :
  CHAN REAL32 carrierToScaleC :
  CHAN REAL32 demodToDataOutC :   
  CHAN REAL32 addToRfScale2C  :  
  CHAN REAL32 gaussToScaleC   :
  
  CHAN REAL32 gaussScaleToAddC    :
  CHAN REAL32 rfOutToAddC    :
  CHAN REAL32 DCarrierC      :
  CHAN REAL32 DCarrierScaleC :
  CHAN REAL32 DDataSrcC      :
  CHAN REAL32 DModulatorC    :
  CHAN REAL32 DDemodulatorC  :
  CHAN REAL32 DRrcFiltC      :
  CHAN REAL32 DRfScaleC      :
  CHAN REAL32 DRfOutC        :
  CHAN REAL32 DGaussScaleC   :
  
  CHAN REAL32 DRfInC         :
  CHAN REAL32 DRfScale2C     :
  CHAN REAL32 DRrcFilt2C     :
  CHAN REAL32 DDataC         :
  CHAN REAL32 DGaussC        : 

  CHAN REAL32 DAddC          :
  
  CHAN REAL32 setupC         :
  CHAN REAL32 setup2C        :
  CHAN REAL32 setup3C        :
  
  PAR
    parameterGen     (setupC!)
    parameterGen     (setup3C!)
    dataSrc          (dataToTxC!,       DDataSrcC!,      setupC?)
    dbpskTransmitter (txToChanFiltC!,   DModulatorC!,    dataToTxC?)
    channelFilter    (filterToScaleC!,  DRrcFiltC!,      txToChanFiltC?)
    rfScale          (scaleToAddC!,     DRfScaleC!,      filterToScaleC?)

    gauss            (gaussToScaleC!,     DGaussC!,        setup3C?)
    gaussScale       (gaussScaleToAddC!, DGaussScaleC!,    gaussToScaleC?)
    add              (addToRfScale2C!,    DAddC!,          scaleToAddC?, gaussScaleToAddC?)

    rfScale2         (scaleToRrcFiltC!, DRfScale2C!,     addToRfScale2C?)
    channelFilter2   (rrcFiltToDemodC!, DRrcFilt2C!,     scaleToRrcFiltC?)
    dbpskReceiver    (demodToDataOutC!, DDemodulatorC!,  rrcFiltToDemodC?)      
    dataOut          (DDataC!,          demodToDataOutC?)

    DoutputToScreenTop (out!, DDataSrcC?, DModulatorC?, DRrcFiltC?, DRfScaleC?, DGaussC?, DGaussScaleC?, DAddC?, DRfScale2C?, DRrcFilt2C?, DDemodulatorC?, DDataC?)  
:
