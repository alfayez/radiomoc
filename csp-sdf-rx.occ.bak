-- Additive Channel
PROC add(CHAN REAL32 outC!, CHAN REAL32 DTopC!, CHAN REAL32 inC?, CHAN REAL32 in2C?)
  REAL32 symbolTime  :
  REAL32 samplingRate:
  REAL32 carrierFreq :
  REAL32 rcFiltCoeff :  -- provides the name of the file with the coeff rcFilt<int>.dat
  REAL32 carrierGain :
  REAL32 rfGain      :
  REAL32 rfGain2     :
  REAL32 carrierPhase :
  REAL32 gaussGain    :
  
  REAL32 seedValG    :
  REAL32 meanValG    :
  REAL32 stdValG     :
  SEQ
    -- We're rcv/fw radio configuration parameters through the channel

    inC  ? symbolTime
    in2C ? symbolTime
    inC  ? samplingRate    
    in2C ? samplingRate    
    inC  ? carrierFreq
    in2C ? carrierFreq
    inC  ? rcFiltCoeff
    in2C ? rcFiltCoeff
    inC  ? carrierGain
    in2C ? carrierGain
    inC  ? rfGain
    in2C ? rfGain
    inC  ? rfGain2
    in2C ? rfGain2    
    inC  ? carrierPhase
    in2C ? carrierPhase
    inC  ? gaussGain
    in2C ? gaussGain    
    inC  ? seedValG
    in2C ? seedValG
    inC  ? meanValG
    in2C ? meanValG
    inC  ? stdValG
    in2C ? stdValG    
    
    
    outC     ! symbolTime
    outC     ! samplingRate
    outC     ! carrierFreq    
    outC     ! rcFiltCoeff
    outC     ! carrierGain
    outC     ! rfGain
    outC     ! rfGain2    
    outC     ! carrierPhase
    outC     ! gaussGain    
    outC     ! seedValG
    outC     ! meanValG
    outC     ! stdValG    
    
    DTopC ! rfGain
:
