-- Define the DBPSK Transmitter Block
PROC dbpskTransmitter(CHAN REAL32 outC!,      CHAN REAL32 DTopC!,
                      CHAN REAL32 inCarrierC?,CHAN REAL32 inDataC?)
  REAL32 symbolTime  :
  REAL32 samplingRate:
  REAL32 carrierFreq :
  REAL32 rcFiltCoeff :  -- provides the name of the file with the coeff rcFilt<int>.dat
  REAL32 carrierGain :
  REAL32 rfGain      :
  REAL32 rfGain2     :
  REAL32 carrierPhase :
  REAL32 gaussGain    :
  
  REAL32 seedValG    :
  REAL32 meanValG    :
  REAL32 stdValG     :
  SEQ
    -- We're rcv/fw radio configuration parameters through the channel

    inCarrierC ? symbolTime
    inDataC    ? symbolTime
    inCarrierC ? samplingRate
    inDataC    ? samplingRate
    inCarrierC ? carrierFreq
    inDataC    ? carrierFreq
    inCarrierC ? rcFiltCoeff
    inDataC    ? rcFiltCoeff
    inCarrierC ? carrierGain
    inDataC    ? carrierGain
    inCarrierC ? rfGain
    inDataC    ? rfGain
    inCarrierC ? rfGain2
    inDataC    ? rfGain2   
    inCarrierC ? carrierPhase
    inDataC    ? carrierPhase    
    inCarrierC ? gaussGain    
    inDataC    ? gaussGain
    inCarrierC ? seedValG
    inDataC    ? seedValG
    inCarrierC ? meanValG
    inDataC    ? meanValG
    inCarrierC ? stdValG    
    inDataC    ? stdValG

    outC       ! symbolTime
    outC       ! samplingRate
    outC       ! carrierFreq
    outC       ! rcFiltCoeff
    outC       ! carrierGain
    outC       ! rfGain
    outC       ! rfGain2    
    outC       ! carrierPhase
    outC       ! gaussGain    
    outC       ! seedValG
    outC       ! meanValG
    outC       ! stdValG    
    
    DTopC      ! rfGain
:

