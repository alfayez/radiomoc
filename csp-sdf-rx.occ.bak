PROC DoutputToScreenTop(CHAN BYTE out!,          CHAN REAL32 rfInC?, 
                       CHAN REAL32 rfScale2C?,                 
                       CHAN REAL32 rrcFilt2C?,   CHAN REAL32 demodulatorC?,  
                       CHAN REAL32 dataC?) 
  
  REAL32 txParam :
  SEQ

    rfInC ? txParam
    out.string("*tRF Input*t", 0, out!)
    out.string("*n", 0, out!)    

    rfScale2C ? txParam
    out.string("*tRF Scale*t", 0, out!)
    out.string("*n", 0, out!)            

    rrcFilt2C ? txParam
    out.string("*tRRC Filt*t", 0, out!)
    out.string("*n", 0, out!)

    demodulatorC ? txParam
    out.string("*tDemodulator*t", 0, out!)
    out.string("*n", 0, out!)    

    dataC ? txParam
    out.string("*tData Out*t", 0, out!)
    out.string("*n", 0, out!)    
